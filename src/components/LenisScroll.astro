<script>
  import "lenis/dist/lenis.css";
  import Lenis from "lenis";

  document.addEventListener("DOMContentLoaded", () => {
    const lenis = new Lenis({
      autoRaf: true,
    });

    function raf(time: number) {
      lenis.raf(time);
      requestAnimationFrame(raf);
    }

    requestAnimationFrame(raf);
  });
</script>
